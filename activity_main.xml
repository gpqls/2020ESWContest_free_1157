<?xml version="1.0" encoding="utf-8"?>

<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <EditText
        android:id="@+id/address"
        android:layout_width="289dp"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="0dp"
        android:hint="Address" />

    <TextView
        android:id="@+id/response"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:layout_x="260dp"
        android:layout_y="537dp" />

    <TextView
        android:id="@+id/signal"
        android:layout_width="259dp"
        android:layout_height="36dp"
        android:layout_x="140dp"
        android:layout_y="489dp"
        android:textAlignment="center"
        android:textSize="27sp"
        tools:layout_editor_absoluteX="dp"
        tools:layout_editor_absoluteY="426dp" />

    <TextView
        android:id="@+id/location"
        android:layout_width="386dp"
        android:layout_height="36dp"
        android:layout_x="13dp"
        android:layout_y="438dp"
        android:textAlignment="center"
        android:textSize="27sp"
        tools:layout_editor_absoluteX="dp"
        tools:layout_editor_absoluteY="426dp" />

    <ImageView
        android:id="@+id/image"
        android:layout_width="386dp"
        android:layout_height="375dp"
        android:layout_x="13dp"
        android:layout_y="51dp"
        tools:layout_editor_absoluteX="283dp"
        tools:layout_editor_absoluteY="426dp" />

    <Button
        android:id="@+id/auto1"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_x="318dp"
        android:layout_y="595dp"
        android:text="↑"
        tools:layout_editor_absoluteX="998dp"
        tools:layout_editor_absoluteY="31dp" />

    <Button
        android:id="@+id/auto2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_x="278dp"
        android:layout_y="635dp"
        android:text="←"
        tools:layout_editor_absoluteX="998dp"
        tools:layout_editor_absoluteY="31dp" />

    <Button
        android:id="@+id/auto3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_x="358dp"
        android:layout_y="635dp"
        android:text="→"
        tools:layout_editor_absoluteX="998dp"
        tools:layout_editor_absoluteY="31dp" />

    <Button
        android:id="@+id/auto4"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_x="318dp"
        android:layout_y="635dp"
        android:text="↓"
        tools:layout_editor_absoluteX="998dp"
        tools:layout_editor_absoluteY="31dp" />

    <Button
        android:id="@+id/connect"
        android:layout_width="112dp"
        android:layout_height="36dp"
        android:layout_x="15dp"
        android:layout_y="485dp"
        android:text="connect"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="998dp"
        tools:layout_editor_absoluteY="31dp" />

    <Button
        android:id="@+id/stop"
        android:layout_width="112dp"
        android:layout_height="36dp"
        android:layout_x="13dp"
        android:layout_y="635dp"
        android:text="STOP"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="998dp"
        tools:layout_editor_absoluteY="31dp" />

    <Button
        android:id="@+id/storeimg"
        android:layout_width="112dp"
        android:layout_height="36dp"
        android:layout_x="13dp"
        android:layout_y="535dp"
        android:text="resotre"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="901dp"
        tools:layout_editor_absoluteY="23dp" />

    <Button
        android:id="@+id/exit"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:layout_x="300dp"
        android:layout_y="13dp"
        android:text="Exit"
        tools:layout_editor_absoluteX="901dp"
        tools:layout_editor_absoluteY="23dp" />

    <Button
        android:id="@+id/showimg"
        android:layout_width="112dp"
        android:layout_height="36dp"
        android:layout_x="13dp"
        android:layout_y="585dp"
        android:text="show image"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="998dp"
        tools:layout_editor_absoluteY="31dp" />

    <Button
        android:id="@+id/manual"
        android:layout_width="112dp"
        android:layout_height="36dp"
        android:layout_x="140dp"
        android:layout_y="585dp"
        android:text="manual"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="998dp"
        tools:layout_editor_absoluteY="31dp" />

    <Button
        android:id="@+id/auto"
        android:layout_width="112dp"
        android:layout_height="36dp"
        android:layout_x="140dp"
        android:layout_y="635dp"
        android:text="auto"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="998dp"
        tools:layout_editor_absoluteY="31dp" />

    <Button
        android:id="@+id/takelocation"
        android:layout_width="112dp"
        android:layout_height="36dp"
        android:layout_x="140dp"
        android:layout_y="535dp"
        android:text="location"
        android:textSize="10dp"
        tools:layout_editor_absoluteX="998dp"
        tools:layout_editor_absoluteY="31dp" />



</AbsoluteLayout>